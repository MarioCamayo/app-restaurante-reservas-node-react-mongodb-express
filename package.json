{
  "name": "reservas-qr-api",
  "version": "1.0.0",
  "description": "API RESTful para reservas con QR",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "babel src -d dist",
    "format": "prettier --write \"src/**/*.js\"",
    "lint": "eslint \"src/**/*.js\"",
    "test": "jest --coverage",
